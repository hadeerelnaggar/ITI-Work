<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:controller="useredit.controllers.UserPageController" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="216.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TitledPane animated="false" collapsible="false" text="Personal info" wrapText="true" BorderPane.alignment="CENTER">
        <content>
          <VBox alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <HBox spacing="65">
                     <children>
                        <Label text="ID" />
                        <TextField fx:id="id" />
                     </children>
                  </HBox>
                   <HBox spacing="65">
                       <children>
                           <Label text="First Name" />
                           <TextField fx:id="firstName" />
                       </children>
                   </HBox>
                   <HBox spacing="65">
                       <children>
                           <Label text="Middle Name" />
                           <TextField fx:id="middleName" />
                       </children>
                   </HBox>
                   <HBox spacing="65">
                       <children>
                           <Label text="Last Name" />
                           <TextField fx:id="lastName" />
                       </children>
                   </HBox>
                   <HBox spacing="65">
                       <children>
                           <Label text="Email" />
                           <TextField fx:id="email" />
                       </children>
                   </HBox>
                   <HBox spacing="65">
                       <children>
                           <Label text="Phone Number" />
                           <TextField fx:id="phone" />
                       </children>
                   </HBox>
                   <HBox alignment="CENTER" spacing="10">
                       <Button fx:id="newUser" onMouseClicked="#addUser" text="New" />
                       <Button fx:id="updateUser" onMouseClicked="#updateUser" text="Update" />
                       <Button fx:id="deleteUser" onMouseClicked="#deleteUser" text="Delete" />
                       <Button fx:id="firstUser" onMouseClicked="#getFirstUser" text="First" />
                       <Button fx:id="previousUser" onMouseClicked="#getPreviousUser" text="Previous" />
                       <Button fx:id="nextUser" onMouseClicked="#getNextUser" text="Next" />
                       <Button fx:id="lastUser" text="Last" onMouseClicked="#getLastUser"/>
                   </HBox>
               </children></VBox>
        </content>
      </TitledPane>
   </center>
</BorderPane>
